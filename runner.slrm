#!/usr/bin/bash
#SBATCH --time=5-0 #time-requested
#SBATCH --mem-per-cpu=12G # memory
##SBATCH --cores-per-socket=8
#SBATCH --output chemReactorGP-%J.log
##First activate Numba because it has a correct numba version

source /home/${USER}/.bashrc
conda activate Numba 

samples=$1 
lQ=$2
Vgain=$3
ratioL=$4
ell=$5
folderName=$6


python chemReactor.py --samples-num=$samples --lQ=$lQ --Vgain=$Vgain --ratio-L=$ratioL --ell=$ell --folderName=$folderName  --resampling=3 --ma-smoother=7

